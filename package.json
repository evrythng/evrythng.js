  "dependencies": {
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "acorn": "^6.1.1",
    "ajv": "^6.10.0",
    "fetch-mock": "^5.8.1",
    "lodash-es": "^4.17.2",
    "webpack": "^4.29.6",
